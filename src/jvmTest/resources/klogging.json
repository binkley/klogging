{
  "sinks": {
    "stdout": {
      "renderWith": "RENDER_ANSI",
      "sendTo": "STDOUT"
    },
    "seq": {
      "seqServer": "http://localhost:5341"
    },
    "splunk": {
      "splunkServer": {
        "hecUrl": "https://localhost:8088",
        "hecToken": "${SPLUNK_HEC_TOKEN}",
        "index": "main",
        "sourceType": "klogging",
        "checkCertificate": "false"
      }
    }
  },
  "logging": [
    {
      "levelRanges": [
        {
          "fromMinLevel": "INFO",
          "toSinks": [
            "stdout",
            "seq",
            "splunk"
          ]
        }
      ]
    }
  ]
}