{
  "sinks": {
    "stdout": {
      "renderWith": "RENDER_ANSI",
      "sendTo": "STDOUT"
    },
    "seq": {
      "seqServer": "http://localhost:5341"
    },
    "splunk": {
      "elkServer": {
        "url": "http://localhost:9200",
        "checkCertificate": "false"
      }
    }
  },
  "logging": [
    {
      "levelRanges": [
        {
          "fromMinLevel": "INFO",
          "toSinks": [
            "stdout",
            "seq",
            "splunk"
          ]
        }
      ]
    }
  ]
}